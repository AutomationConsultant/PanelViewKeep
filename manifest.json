{
	"name": "Panel View for Google Keep",
	"description": "Panelized extension for Google Keep",
	"version": "1.7",
	"background": {
		"scripts": [
			"js/background.js",
			"js/events.js",
			"js/contextmenu.js"
		]
	},
	"browser_action": {
		"default_icon": "img/icon_16.png"
	},
	"icons": {
		"128": "img/icon_128.png",
		"16": "img/icon_16.png"
	},
	"permissions": [
		"tabs",
		"storage",
		"contextMenus",
		"https://drive.google.com/keep/*"
	],
	"content_scripts": [
		{
			"js": [
				"js/jquery-1.7.2.min.js"
			],
			"matches": [
				"https://drive.google.com/keep/*"
			],
			"run_at": "document_start"
		},
		{
			"css": [
				"css/keep-style.css"
			],
			"js": [
				"js/keep-script.js"
			],
			"matches": [
				"https://drive.google.com/keep/*"
			],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": [
		"html/dialogs.html"
	],
	"manifest_version": 2
}
